<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>
<sqlMap namespace="map">
  <typeAlias alias="dto" type="map.mapDto"/>


<insert id="insertInfo" parameterClass="dto">
  insert into basicCourseInfo(category,num,id,pass1name,pass1xy,pass2name,pass2xy,pass3name,pass3xy,pass4name,pass4xy,pass5name,pass5xy,regdate)
           values(#category#,basicCourseInfo_seq.NEXTVAL,#id#,#pass1name#,#pass1xy#,#pass2name#,#pass2xy#,#pass3name#,#pass3xy#,#pass4name#,#pass4xy#,#pass5name#,#pass5xy#,sysdate)
</insert>
</sqlMap>